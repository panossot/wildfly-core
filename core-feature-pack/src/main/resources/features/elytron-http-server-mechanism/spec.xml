<?xml version="1.0" encoding="UTF-8"?>
<feature-spec name="elytron-http-server-mechanism" xmlns="urn:wildfly:pm-feature-spec:1.0">
  <annotation name="add">
    <elem name="addr-params" value="host,profile,subsystem,provider-http-server-mechanism-factory"/>
  </annotation>
  <refs>
    <ref feature="elytron"/>
  </refs>
  <params>
    <param name="host" default="PM_UNDEFINED" feature-id="true"/>
    <param name="profile" default="PM_UNDEFINED" feature-id="true"/>
    <param name="subsystem" feature-id="true"/>
    <param name="provider-http-server-mechanism-factory" default="global" feature-id="true"/>
  </params>
</feature-spec>
